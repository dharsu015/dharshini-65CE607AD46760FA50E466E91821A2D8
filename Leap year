def checkleap (year):
# checking  if the given year is leap year
if((year % 400 == 0)or
(year % 100 ! =0) and
(year % 4== 0)):
print("given year is a leap year");
#else it is not a leap year
else:
print("given year is not a leap year")
#taking an input year from user
year = int(input("enter the number :"))
# printing result
check leap(year)
